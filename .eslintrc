{
  "extends": ["airbnb-base", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": ["error"],
    "consistent-return": "off",
    "no-restricted-syntax": "off",
    "no-plusplus": "off",
    "no-await-in-loop": "off"
  },
  "env": {
    "node": true,
    "jest":true
  },
  "parserOptions": {
    "ecmaVersion": 2020
  }
}
